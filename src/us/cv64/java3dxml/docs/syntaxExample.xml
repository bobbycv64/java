<universe>
  	<camera>
		<translate x="0" y="0" z="10" />
	</camera>

	<lights>
		<light name="light0">
			<color red="255" green="255" blue="255" />
			<translate x="0" y="0" z="10" />
		</light>

		<light name="light1">
			<color red="0" green="0" blue="255" />
			<translate x="0" y="10" z="0" />
		</light>
	</lights>

	<scene>
		<xyz name="coordinateSystem0">
			<translate x="50" y="0" z="0" />

			<object name="sphere0">
				<sphere diameter="1">
					<color red="255" green="255" blue="255" />
					<translate x="0" y="0" z="0" />
				</sphere>
			</object>

			<object name="cube0">
				<cube length="1" width="1" height="1">
					<color red="255" green="255" blue="255" />
					<translate x="5" y="5" z="0" />
					<scale x="0.5" y="0.5" z="0" />
					<rotate x="45" y="0" z="0" />
					<shere x="0" y="10" z="0" />
				</cube>
			</object>
		</xyz>

		<coordinateSystem name="coordinateSystem1">
			<translate x="50" y="0" z="0" />

			<object name="sphere0">
				<sphere diameter="10">
					<color red="0" green="255" blue="0" />
					<translate x="0" y="0" z="0" />
				</sphere>
			</object>

			<object name="cube0">
				<cube length="10" width="10" height="10">
					<color red="255" green="255" blue="255" />
					<translate x="5" y="5" z="0" />
					<scale x="0.5" y="0.5" z="0" />
					<rotate x="45" y="0" z="0" />
					<shere x="0" y="10" z="0" />
				</cube>
			</object>
		</coordinateSystem>
	</scene>
</universe>